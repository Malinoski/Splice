        <term_definitions code="at1008">
          <items id="description">Uma braçadeira usada para adultos pequenos -manguito de aproximadamente 10cm x 24cm.</items>
          <items id="text">Adulto pequeno</items>
        </term_definitions>
        <term_definitions code="at1009">
          <items id="description">Manguito apropriao para uma criança ou um adulto com um braço fino - bolsa de aproximadamente 8cm x 21cm.</items>
          <items id="text">Criança/Pediátrico</items>
        </term_definitions>
        <term_definitions code="at1010">
          <items id="description">Registro  do som Korotkoff usado para determinar a pressão arterial diastólica, usando o método auscultativo.</items>
          <items id="text">Final da diástole</items>
        </term_definitions>
        <term_definitions code="at1011">
          <items id="description">O quarto som de  Korotkoff é  identificado como um súbito abafamento dos sons.</items>
          <items id="text">Fhase IV.</items>
        </term_definitions>
        <term_definitions code="at1012">
          <items id="description">O quinto som de Korotkoff é identificado pela ausência de sons,  pois a pressão da braçadeira cai abaixo da pressão diastólica do sangue.</items>
          <items id="text">Phase V</items>
        </term_definitions>
        <term_definitions code="at1014">
          <items id="description">Deitado sem reclinação com alguma inclinação lateral, usualmente com angulação para o lado esquerdo. Comumente requerido no último trimestre da gravidez para aliviar a compressão aortocaval.</items>
          <items id="text">Deitado com inclinação para esquerda</items>
        </term_definitions>
        <term_definitions code="at1018">
          <items id="description">Uma braçadeira utilizada em crianças  pequenas - manguito de aproximadamente 5cm x 15cm.</items>
          <items id="text">Criança pequena</items>
        </term_definitions>
        <term_definitions code="at1019">
          <items id="description">Uma braçadeira usada para um recém-nascido, supondo que o tamanho é apropriado para a maturidade e o peso ao nascer do neonato.</items>
          <items id="text">Neonatal</items>
        </term_definitions>
        <term_definitions code="at1020">
          <items id="description">O pulso direito da pessoa.</items>
          <items id="text">Pulso direito</items>
        </term_definitions>
        <term_definitions code="at1021">
          <items id="description">O pulso esquerdo da pessoa.</items>
          <items id="text">Pulso esquerdo</items>
        </term_definitions>
        <term_definitions code="at1025">
          <items id="description">Detalhes sobre o esfigmomanômetro ou outro aparelho utilizado para medir a pressão sanguínea.</items>
          <items id="text">Aparelho</items>
        </term_definitions>
        <term_definitions code="at1026">
          <items id="description">O tornozelo direito da pessoa.</items>
          <items id="text">Tornozelo direito</items>
        </term_definitions>
        <term_definitions code="at1030">
          <items id="description">Detalhes sobre atividade física realizada na hora da medida da pressão arterial.</items>
          <items id="text">Esforço físico</items>
        </term_definitions>
        <term_definitions code="at1031">
          <items id="description">O tornozelo esquerdo da pessoa.</items>
          <items id="text">Tornozelo esquerdo</items>
        </term_definitions>
        <term_definitions code="at1032">
          <items id="description">O dedo da mão da pessoa. A identificação do dedo pode ser registrada no elemento de dado 'Local Específico', se requerido.</items>
          <items id="text">Dedo da mão</items>
        </term_definitions>
        <term_definitions code="at1033">
          <items id="description">Local do corpo onde  foi medida a pressão arterial.  Use  'Local da Medida' para selecionar entre os locais mais comuns.  Use 'Local Específico' para registrar detalhes mais específicos ou um local que não está representado no conjunto comum ou para se referenciar a uma terminologia externa.</items>
          <items id="text">Local</items>
        </term_definitions>
        <term_definitions code="at1034">
          <items id="description">Detalhes específicos sobre o local do corpo onde a pressão arterial é registrada.</items>
          <items id="text">Local Específico</items>
        </term_definitions>
        <term_definitions code="at1035">
          <items id="description">Método de medida da pressão arterial.</items>
          <items id="text">Método</items>
        </term_definitions>
        <term_definitions code="at1036">
          <items id="description">Método de medir a pressão externamente, usando o estetoscópiso e os sons Korotkoff.</items>
          <items id="text">Ausculta</items>
        </term_definitions>
        <term_definitions code="at1037">
          <items id="description">Método de medir a pressão arterial externamente, usando a palpação (geralmente artérias braquiais ou radiais).</items>
          <items id="text">Palpação</items>
        </term_definitions>
        <term_definitions code="at1038">
          <items id="description">Fórmula usada para calcular a pressão arterial média (PAM), se registrada em dados.</items>
          <items id="text">Fórmula de Pressão Arterial Média</items>
        </term_definitions>
        <term_definitions code="at1039">
          <items id="description">Método de medir a pressão arterial externamente, usando o aparelho de pressão arterial.</items>
          <items id="text">Aparelho de pressão</items>
        </term_definitions>
        <term_definitions code="at1040">
          <items id="description">Método de medir a pressão arterial internamente, isto é, envolvendo a penetração da pele e a medida interior dos vasos sanguíneos.</items>
          <items id="text">Invasivo</items>
        </term_definitions>
        <term_definitions code="at1042">
          <items id="description">Estimativa da pressão arterial média em um período de 24 horas.</items>
          <items id="text">Media de 24 horas</items>
        </term_definitions>
        <term_definitions code="at1043">
          <items id="description">Status do sono - apóia a interpretação do registro do mapa de pressão arterial (ambulatorial de 24 horas).</items>
          <items id="text">Status do sono</items>
        </term_definitions>
        <term_definitions code="at1044">
          <items id="description">Sujeito está totalmente consciente.</items>
          <items id="text">Alerta e acordado</items>
        </term_definitions>
        <term_definitions code="at1045">
          <items id="description">Sujeito está no estado natural de descanso corporal.</items>
          <items id="text">Dormindo</items>
        </term_definitions>
        <term_definitions code="at1051">
          <items id="description">O dedo do pé da pessoa. A identificação do dedo do pé pode ser registrada  no elemento de dado 'Local Específico', se requerido.</items>
          <items id="text">Dedo do pé</items>
        </term_definitions>
        <term_definitions code="at1052">
          <items id="description">Comentários sobre e registros de outros fatores incidentais que possam estar contribuindo na medida da pressão sanguínea. Por exemplo, nível de ansiedade ou "síndrome do jaleco branco"; dor ou febre; mudanças na pressão atmosférica, etc.</items>
          <items id="text">Fatores confundidores</items>
        </term_definitions>
        <term_definitions code="at1053">
          <items id="description">Medida invasiva da pressão em uma artéria acessada  por um cateter . Local do transductor pode ser registrado no elemento de dado  'Local Específico', se requerido.</items>
          <items id="text">Intra-arterial</items>
        </term_definitions>
